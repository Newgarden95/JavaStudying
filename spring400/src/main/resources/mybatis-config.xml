<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
  <!-- 매퍼파일들을 관리해줄 xml파일 -->
<configuration>
   <typeAliases>
      <typeAlias type="com.mega.mvc400.CartVO" alias="cartVO"></typeAlias> <!-- alias = cartVO는 매퍼에서 사용하는 VO -->
   </typeAliases>
   <mappers>
      <mapper resource="cartMapper.xml" /> <!-- sql문이 저장된 xml파일을 매퍼로 사용 -->
   </mappers>
</configuration>