package 배열;

public class 실습2_3 {
	public static void main(String[] args) {
		int[] a = {1,2,3,4,5};
		int[] b = a.clone(); //b는 a의 배열을 복제하여 참조
		
		b[3] = 0;
		

	  System.out.print("\nb = ");
	  for (int i = 0; i < b.length; i++) {
		System.out.print(" "+b[i]);
	}
	  System.out.println();
	  System.out.print("a = ");
	  for (int i = 0; i < a.length; i++) {
		System.out.print(" "+a[i]);
	}
	}
}
